var start = @time.utc_milli();
for(var i = 1e8;i>0;){
    i = i-1;
}
print @time.utc_milli() - start;

start = @time.utc_milli();
for(var i = 0;i<1e8;){
    i = i+1;
}
print @time.utc_milli() - start;

start = @time.utc_milli();
for(var i = 0,j = 1;i<1e8;){
    i = i+j;
}
print @time.utc_milli() - start;