fun createPoint(x,y){
    fun pointerFuc(funName){
        if(funName == "x"){
            return x;
        } else if(funName == "y"){
            return y;
        }
    }

    return pointerFuc;
}

fun stress() {
    @system.gcBegin(256 * 1024);

    var count = 1e6;
    while (count > 0) {
        createPoint(count, count)("x");
        count = count - 1;
    }
}

stress();